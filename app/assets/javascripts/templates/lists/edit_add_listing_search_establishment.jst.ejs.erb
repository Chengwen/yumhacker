<div class='establishment_info'>
    [[ if (get('path')) { ]]
        <a class='biz_name' href='[[= "/" + get("path") ]]'>[[= get('name') ]]</a>
    [[ } else { ]]
        <div class='biz_name'>[[= get('name') ]]</div>
    [[ } ]]

    <div class='details'>[[= get('formatted_address') ]]</div>
</div>

<div class='establishment_action_btn_container'>
    [[ if (get('listed')) { ]]
        <a class='engaged btn text'>Listed</a>
    [[ } else { ]]
        <a class='add_btn btn text'>Add to List</a>
    [[ } ]]
</div>

<br class='clear'>